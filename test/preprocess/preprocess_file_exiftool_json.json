[
    {
        "name": "preprocess asset _res/assets/camera.jpg with exiftool in json format",
        "request": {
            // internal HTTP endpoint /load-file: returns header in response body
            "endpoint": "load-file",
            "method": "GET",
            "query_params": {
                "file": "assets/camera.jpg"
            }
        },
        "response": {
            "format": {
                "pre_process": {
                    "cmd": {
                        "name": "exiftool",
                        "args": [
                            "-j",
                            "-g",
                            "-"
                        ]
                    }
                }
            },
            "body": [
                {
                    "Composite": {},
                    "ExifTool": {},
                    "File": {
                        "FileType": "JPEG",
                        "FileTypeExtension": "jpg",
                        "MIMEType": "image/jpeg"
                    },
                    "ICC_Profile": {},
                    "JFIF": {}
                }
            ]
        }
    }
]