[
    {
        "name": "GET /session for \"root\"",
        "request": {
            "endpoint": "session",
            "method": "GET",
            "header":{
                "x-easydb-token": ""
            }
        },
        "store_response_qjson": {
            "root-token": "body.token"
        }
    },
    {
        "name": "POST /session/authenticate for \"root\"",
        "request": {
            "endpoint": "session/authenticate",
            "method": "POST",
            "query_params": {
                "method": "easydb",
                "login": "root",
                "password": "admin"
            },
            "header_from_store": {
                "x-easydb-token": "root-token"
            }
        },
        "store_response_qjson": {
            "maxEventID": "body.current_max_event_id"
        }
    }
]